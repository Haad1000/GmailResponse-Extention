{
    "manifest_version": 3,
    "name": "EmAIl",
    "version": "1.0.0",
    "description": "",
    "permissions": ["activeTab","scripting", "storage", "tabs"],
    "host_permissions": ["https://mail.google.com/*"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://mail.google.com/*"],
            "js": ["popup.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["scripts/inject.js"],
            "matches": ["https://mail.google.com/*"]
        }
    ]
}